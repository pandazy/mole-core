{
	"defaultSeverity": "error",
	"extends": ["tslint:latest", "tslint-config-prettier"],
	"jsRules": {},
	"rules": {
		"max-line-length": {
			"options": 140
		},
		"indent": [true, "tabs", 2],
		"quotemark": [true, "single", "avoid-escape"],
		"interface-name": [false],
		"object-literal-sort-keys": [false],
		"no-trailing-whitespace": true
	},
	"linterOptions": {
		"exclude": ["**/*.spec.ts"]
	},
	"rulesDirectory": []
}
